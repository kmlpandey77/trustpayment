{
    "name": "kmlpandey77/trustpayment",
    "description": "This is my package trustpayment",
    "keywords": [
        "kmlpandey77",
        "laravel",
        "trustpayment"
    ],
    "homepage": "https://github.com/kmlpandey77/trustpayment",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamal Pandey.",
            "email": "kmlpandey77@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Kmlpandey77\\Trustpayment\\": "src",
            "Kmlpandey77\\Trustpayment\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kmlpandey77\\Trustpayment\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kmlpandey77\\Trustpayment\\TrustpaymentServiceProvider"
            ],
            "aliases": {
                "Trustpayment": "Kmlpandey77\\Trustpayment\\Facades\\Trustpayment"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
